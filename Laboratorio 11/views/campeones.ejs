<%- include('partials/head.ejs') %>
<%- include('partials/navbar.ejs') %>

    <main class="container">
      <h1><%= titulo %></h1>

      <div class="row">
      <% for (let personaje of lista_personajes){ %>
        <div class="col s4">
        <div class="card">
          <div class="card-image waves-effect waves-block waves-light">
            <img class="activator" src="https://www.egames.news/__export/1617404147991/sites/debate/img/2021/04/02/cuxles_son_las_habilidades_de_gwen_en_league_of_legends.jpg_423682103.jpg">
          </div>
          <div class="card-content">
            <span class="card-title activator grey-text text-darken-4"><%= personaje %><i class="material-icons right">more_vert</i></span>
            <p><a href="#">...</a></p>
          </div>
          <div class="card-reveal">
            <span class="card-title grey-text text-darken-4"><%= personaje %><i class="material-icons right">close</i></span>
            <p>Aquí habrá más información del personaje.</p>
          </div>
        </div>
        </div>  
      <% } %>
    </div>

    </main>

<%- include('partials/footer.ejs') %>